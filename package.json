{
  "name": "BrowserLogger",
  "version": "1.0.0",
  "description": "Clientside_logger to capture client side error information during runtimme",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint -c lib/**/*.js",
    "transpile": "babel lib --out-dir dist --copy-files --ignore='**/test/*.js'",
    "babelify": "cross-env NODE_ENV=production node ./node_build/build.js",
    "build": "npm run lint && npm run babelify"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/raghuraghavan/clientside_logger.git"
  },
  "files": [
    "readme.md",
    "dist",
    "doc"
  ],
  "keywords": [
    "Client",
    "side",
    "logger"
  ],
  "author": "Raghu Raghavan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/raghuraghavan/clientside_logger/issues"
  },
  "homepage": "https://github.com/raghuraghavan/clientside_logger#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-syntax-json-strings": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.3",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "eslint-rules-es2015": "^1.0.0",
    "nodemon": "^1.18.9",
    "uglify-js": "^3.4.9"
